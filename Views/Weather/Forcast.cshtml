@model string

@{
    ViewData["Title"] = "Forcast";
}

<form asp-action="Forcast">
    <label>CityName</label>
    <input type="text" id="name" name="newCityName">

    <button type="submit" class="btn btn-success">Search</button>
</form>

@(await Component.InvokeAsync("CityTemperature", new {newCityName = ViewData["NewCityName"]?.ToString()}))